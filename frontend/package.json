{
  "name": "adstxt-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^5.3.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^16.18.126",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "aws-amplify": "^5.3.27",
    "axios": "^1.8.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "DANGEROUSLY_DISABLE_HOST_CHECK=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios|@aws-amplify)/\"",
    "eject": "react-scripts eject",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "verify": "npm run typecheck && npm run lint",
    "prebuild": "npm run typecheck"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000",
  "devDependencies": {
    "ts-jest": "^29.3.0",
    "typescript": "^4.9.5"
  }
}
