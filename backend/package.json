{
  "name": "adstxt-manager-backend",
  "version": "1.0.0",
  "description": "Backend for Ads.txt Manager application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "migrate": "ts-node src/db/migrations/run.ts",
    "migrate:sellers": "ts-node src/db/migrations/run_sellers_json.ts",
    "migrate:pg": "ts-node src/db/migrations/postgresql-setup.ts",
    "migrate:pg:force": "ts-node src/db/migrations/postgresql-setup.ts --migrate",
    "seed": "ts-node src/db/seeds/run.ts",
    "fetch-sellers-json": "node fetch-sellers-json.js"
  },
  "keywords": [
    "ads.txt",
    "publisher",
    "advertising"
  ],
  "author": "Yoshihiko Miyaichi",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "i18next": "^24.2.3",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "psl": "^1.15.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stream-json": "^1.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.11",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
